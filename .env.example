# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Google Gemini (Primary AI for meal scanning)
# Get your free API key at https://aistudio.google.com/apikey
GOOGLE_GEMINI_API_KEY=your_gemini_api_key

# LogMeal (Fallback AI - optional, used when Gemini hits rate limits)
# Get your free API key at https://logmeal.com/api/
LOGMEAL_API_KEY=your_logmeal_api_key

# Open Food Facts (no key needed, but good to document)
OPEN_FOOD_FACTS_USER_AGENT=NutriTrack/1.0

# App
NEXT_PUBLIC_APP_URL=http://localhost:3000
